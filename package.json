    "typescript": "^4.6.3",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-hardcore": "^24.10.0"
