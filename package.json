{
  "name": "finki-discord-bot",
  "version": "1.0.0",
  "description": "FINKI Discord bot",
  "main": "src/main.js",
  "scripts": {
    "start": "tsc && node dist/main.js",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "embed:colors": "tsc && node dist/scripts/colors.js",
    "embed:years": "tsc && node dist/scripts/years.js",
    "embed:activities": "tsc && node dist/scripts/activities.js",
    "embed:courses": "tsc && node dist/scripts/courses.js",
    "embed:programs": "tsc && node dist/scripts/programs.js",
